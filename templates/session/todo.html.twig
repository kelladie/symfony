{% extends 'template.html.twig' %}
{% block title %}Hello SessionController!
{% endblock %}


{% block body %}
	<style>
		.example-wrapper {
			margin: 1em auto;
			max-width: 800px;
			width: 95%;
			font: 18px / 1.5 sans-serif;
		}
		.example-wrapper code {
			background: #F5F5F5;
			padding: 2px 6px;
		}
	</style>

	<div class="example-wrapper">


		{% block pageTitle %}Liste des todos
		{% endblock %}
		<ol class="list-group">
			{% for key,txt in app.session.get('todos') %}

				<li class="list-group-item">
					{{key}}:{{txt}}</li>


			{% endfor %}
		</ol>


	</div>
{% endblock %}
